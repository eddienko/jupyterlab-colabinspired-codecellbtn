{
  "name": "@eddienko/jupyterlab-colabinspired-codecellbtn",
  "version": "0.1.0",
  "description": "Extension for adding buttons to each code cell",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "license": "BSD-3-Clause",
  "author": "Eduardo Gonzalez Solares",
  "repository": {
    "type": "git",
    "url": "https://github.com/eddienko/jupyterlab-colabinspired-codecellbtn"
  },
  "main": "lib/index.js",
  "style": "style/index.css",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project tsconfig.json",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/apputils": "^2.0.0",
    "@jupyterlab/cells": "^2.0.0",
    "@jupyterlab/notebook": "^2.0.0",
    "@lumino/coreutils": "^1.4.2",
    "@lumino/commands": "^1.10.1",
    "@types/node": "^12.7.11",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "jupyterlab": {
    "extension": true
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "singleQuote": true
  }
}
